programa {
  funcao inicio() {
    cadeia  nome, telefone

    escreva("digite o nome da pessoa:")
    leia(nome)

    escreva("digite o numero de telefone:")
    leia(telefone)

    escreva("nome da pessoa :" ,  nome )

    escreva("numero de telefone:", telefone )

    escreva("contato:" ,nome, " - telefone:", telefone )
    
  }
}
